{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "group",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "minLength": 2,
      "pattern": "^[^-\\s][a-zA-Z0-9_\\s-]+$"
    },
    "dueDate": {
      "type": "string"
    }
  },
  "required": [
    "name",
    "dueDate"
  ],
  "additionalProperties": false
}
